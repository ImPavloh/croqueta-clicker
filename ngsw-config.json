{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/manifest.webmanifest", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**/*.svg",
          "/assets/**/*.cur",
          "/assets/**/*.jpg",
          "/assets/**/*.jpeg",
          "/assets/**/*.png",
          "/assets/**/*.apng",
          "/assets/**/*.webp",
          "/assets/**/*.avif",
          "/assets/**/*.gif",
          "/assets/**/*.otf",
          "/assets/**/*.ttf",
          "/assets/**/*.woff",
          "/assets/**/*.woff2",
          "/assets/**/*.mp3",
          "/assets/**/*.ogg",
          "/assets/**/*.wav",
          "/assets/i18n/*.json"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "leaderboard-api",
      "urls": ["https://cosvywlgllqpnqdrphtt.supabase.co/rest/v1/leaderboard*"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 20,
        "maxAge": "15m",
        "timeout": "5s"
      }
    }
  ]
}
